{
  "name": "electron-pdf-viewer",
  "productName": "Electron PDF Viewer",
  "applicationId": "com.alex313031.electron-pdf-viewer",
  "version": "1.2.4",
  "description": "A PDF viewer built with Electron and PDF.js",
  "main": "./app/main.js",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "start": "electron .",
    "dev": "npm run lint && cross-env NODE_ENV=development ELECTRON_ENABLE_LOGGING=1 electron --enable-logging=file --user-data-dir=./test ./app",
    "build": "electron-builder --dir",
    "dist": "electron-builder",
    "distclean": "rimraf dist",
    "clean": "rimraf dist/ && rimraf test/ && rimraf app/node_modules/ && rimraf node_modules/ && cd ./pdf.js && npm run clean",
    "burn": "rimraf package-lock.json && rimraf app/package-lock.json",
    "test": "npm run eslint ; npm run stylelint",
    "lint": "npm run eslint ; npm run stylelint",
    "eslint": "eslint \"app/**/*.js*\"",
    "stylelint": "stylelint \"app/**/*.css*\"",
    "postinstall": "electron-builder install-app-deps"
  },
  "author": "Alex313031 <alex313031@gmail.com>",
  "license": "MIT",
  "copyright": "Copyright Â© 2024 Alex313031",
  "website": "https://thorium.rocks",
  "homepage": "https://github.com/Alex313031/electron-pdf-viewer#readme",
  "repository": "https://github.com/Alex313031/electron-pdf-viewer",
  "bugs": {
    "url": "https://github.com/Alex313031/electron-pdf-viewer/issues"
  },
  "keywords": [
    "Electron",
    "PDF",
    "viewer",
    "PDF.js"
  ],
  "dependencies": {
    "@electron/remote": "2.1.2",
    "electron-context-menu": "3.6.1",
    "electron-log": "5.1.1",
    "electron-store": "8.1.0"
  },
  "devDependencies": {
    "asar": "3.2.0",
    "cross-env": "7.0.3",
    "electron": "22.3.27",
    "electron-builder": "23.6.0",
    "eslint": "8.44.0",
    "rimraf": "3.0.2",
    "minimist": "1.2.8",
    "normalize-url": "8.0.0",
    "stylelint": "14.16.1",
    "stylelint-config-standard": "29.0.0"
  }
}
